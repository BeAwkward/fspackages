	<Template Name="WT_Detached_Control_Panel_Template">
		<DefaultTemplateParameters>
			<AS3000>AS3000</AS3000>
			<DISPLAY>MFD</DISPLAY>
			<TYPE></TYPE>
			<ID>1</ID>
			<ANIM_NEVER_FAILS/>
		</DefaultTemplateParameters>

		<UseTemplate Name="ASOBO_GT_Helper_Suffix_ID_Appender">
